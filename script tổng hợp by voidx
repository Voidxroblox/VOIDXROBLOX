local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "VOIDX & grox Hub",
   LoadingTitle = "VOIDX & grox",
   LoadingSubtitle = "by voidx",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "VOIDXgroxConfig",
      FileName = "Settings"
   },
   KeySystem = false,
   Theme = "Amethyst"  -- Theme tím neon đẹp lung linh
})

-- Tab Blox Fruits
local BloxFruitsTab = Window:CreateTab("Blox Fruits", "apple")

BloxFruitsTab:CreateButton({
   Name = "QuantumOnyx",
