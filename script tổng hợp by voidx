-- Tab Auto Bounty
local BountyTab = Window:CreateTab("Auto Bounty", "dollar-sign")

BountyTab:CreateButton({
   Name = "Auto Bounty (SeraHub)",
   Callback = function()
      getgenv().config = {
         ["Team"] = "Pirates",
         ["Use Race"] = true,
         ["MinBountyHunt"] = 0,
         ["MaxBountyHunt"] = 30000000,
         ["SafeHealth"] = 4000
      }
      loadstring(game:HttpGet("https://raw.githubusercontent.com/LumosSera/SeraHub/main/AutoBounty.lua"))()
   end
})

